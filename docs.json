{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "palm",
  "name": "MachinaOs",
  "colors": {
    "primary": "#BD93F9",
    "light": "#FF79C6",
    "dark": "#6272A4"
  },
  "logo": {
    "dark": "/logo/dark.svg",
    "light": "/logo/light.svg"
  },
  "favicon": "/favicon.svg",
  "modeToggle": {
    "default": "dark"
  },
  "navigation": {
    "dropdowns": [
      {
        "dropdown": "Documentation",
        "icon": "book",
        "groups": [
          {
            "group": "Getting Started",
            "pages": ["introduction", "installation", "quickstart"]
          },
          {
            "group": "Tutorials",
            "pages": [
              "tutorials/first-workflow",
              "tutorials/ai-agent-workflow",
              "tutorials/whatsapp-automation",
              "tutorials/android-automation"
            ]
          },
          {
            "group": "Node Catalog",
            "pages": [
              "nodes/overview",
              "nodes/ai-models",
              "nodes/ai-agent",
              "nodes/skills",
              "nodes/tools",
              "nodes/whatsapp",
              "nodes/android",
              "nodes/documents",
              "nodes/webhooks",
              "nodes/schedulers"
            ]
          },
          {
            "group": "Deployment",
            "pages": ["deployment/docker", "deployment/production"]
          },
          {
            "group": "FAQ",
            "pages": ["faq"]
          }
        ]
      }
    ]
  },
  "navbar": {
    "links": [
      {
        "label": "GitHub",
        "href": "https://github.com/trohitg/MachinaOs"
      }
    ]
  },
  "footerSocials": {
    "github": "https://github.com/trohitg/MachinaOs"
  },
  "feedback": {
    "thumbsRating": true
  },
  "search": {
    "prompt": "Search MachinaOs docs..."
  },
  "styling": {
    "codeBlock": {
      "theme": {
        "dark": "dracula",
        "light": "github-light"
      }
    }
  }
}
